<div class="w-75 mx-auto mb-5 mt-5">
	<div class="back-btn mb-4">
		<a href="/admin" class="text-dark"><i class="fas fa-chevron-left"></i> Back</a>
	</div>
	{{#if isEmptyList }}
	<div class="adm-orders-table-legend">
		<div class="row align-items-start border-bottom">
			<div class="col">
				User Id
			</div>
			<div class="col">
				Customer Name
			</div>
			<div class="col">
				Email
			</div>
			<div class="col">
				Phone
			</div>
			<div class="col">
				Address
			</div>
			<div class="col">
				Nickname
			</div>
		</div>
	</div>

	<div class="adm-orders-table">
		{{#each users}}
		<div class="row align-items-start mt-4 mb-4 border-bottom">
			<div class="col">
				#{{@key}}
			</div>
			<div class="col">
				{{this.custName}}
			</div>
			<div class="col">
				{{this.email}}
			</div>
			<div class="col">
				{{this.phone}}
			</div>
			<div class="col">
				{{this.address}}
			</div>
			<div class="col">
				{{this.nickname}}
			</div>
		</div>
		{{/each}}
	</div>

	{{else}}
	<div class="empty-errList mt-5">
		<p class="text-center fw-bold fs-5">Currently, we don't have any users. ðŸ˜¥</p>
	</div>
	{{/if }}
</div>